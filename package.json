{
  "name": "44billion",
  "author": "Arthur Fran√ßa",
  "version": "1.0.0",
  "description": "Notsr App (napp) Launcher",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "_start": "NODE_ENV=development npm run _build & NODE_ENV=development node server/dev-server.js",
    "start": "cd ../44b-vault && npm run start & npm run _start",
    "build": "NODE_ENV=production npm run _build",
    "_build": "node bin/build.js",
    "test": "node --test 'tests/**/*.test.js'",
    "test:only": "node --test --test-only 'tests/**/*.test.js'"
  },
  "devDependencies": {
    "core-js": "^3.45.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.22.0",
    "eslint-plugin-html": "^8.1.2",
    "globals": "^15.12.0",
    "neostandard": "^0.12.1"
  },
  "imports": {
    "#f": "thenameisf",
    "#f/*": "thenameisf/*",
    "#bin/*": "./bin/*",
    "#server/*": "./server/*",
    "#*": [
      "./src/*"
    ],
    "#assets/*": "./src/assets/*",
    "#config/*": "./src/config/*",
    "#helpers/*": "./src/helpers/*",
    "#scripts/*": "./src/scripts/*",
    "#services/*": "./src/services/*",
    "#service-workers/*": "./src/service-workers/*",
    "#hooks/*": "./src/components/hooks/*",
    "#shared/*": "./src/components/shared/*",
    "#views/*": "./src/components/views/*",
    "#zones/*": "./src/components/zones/*"
  },
  "dependencies": {
    "@scure/base": "^2.0.0",
    "mime": "^4.0.7",
    "nostr-tools": "^2.16.2",
    "thenameisf": "^1.0.2",
    "url-router": "^13.0.0"
  }
}
